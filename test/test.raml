version: 1
baseUri: http://example.com
types:
  language:
    type: string
    description: A programming language name
    enum: [Ruby, Python, JavaScript]
mediaType: [application/json, application/xml]
/test:
  get:
    responses:
      200:
        headers:
          Cache-Control:
          Expires:
            type: datetime
        body:
          application/json:
          application/xml:
      404:
        body:
          properties:
            statusCode: string
            message: string
      406:
        body:
          type: language
  post:
    responses:
      200:
        description: test
  put:
    responses:
      200:
